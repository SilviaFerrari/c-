#include <iostream>
#include <string>
using namespace std;
string thing;
int lenght;
int main() {
	cout << "inserisci la lunghezza della lista" << endl;
	cin >> lenght;
	string things[lenght] = {};
	cout << "Scrivi gli elmenti da aggiungere alla lista" << endl;
	for(int i = 0; i < lenght; i++) {
    	cin >> thing;
    	things[i] = thing;
	}
	cout << "Ecco la tua lista:" << endl;
  	for(int i = 0; i < lenght; i++) {
    	cout << things[i] << endl;
	}
  return 0;
}
